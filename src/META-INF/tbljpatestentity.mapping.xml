<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">
    <entity class="com.java.dao.TblJpatestEntity">
        <table name="TBL_JPATEST"/>
        <attributes>
            <id name="id">
                <generated-value strategy="SEQUENCE" generator = "TBL_JPATEST_SEQGEN"/>
                <sequence-generator name="TBL_JPATEST_SEQGEN" sequence-name="TBL_JPATEST_SEQ" allocation-size="1"/>
            </id>
            <basic name="name">
                <column name="NAME"/>
            </basic>
            <basic name="rdate">
                <column name="RDATE"/>
            </basic>
            <basic name="ramount">
                <column name="RAMOUNT"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
