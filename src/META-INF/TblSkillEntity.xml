<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.java.entity.Skill">
        <table name="TBL_SKILL" schema="CAASAA"/>
        <attributes>
            <id name="skillId">
                <column name="SKILL_ID"/>
            </id>
            <basic name="skillcategory">
                <column name="SKILLCATEGORY"/>
            </basic>
            <basic name="skill">
                <column name="SKILL"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
